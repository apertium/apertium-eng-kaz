<clip pos="2" part="lem"/>
 <clip pos="2" part="tags"/>
 <clip pos="2" part="chcontent"/>
          <!--  <clip pos="2" part="lem"/>
            <lit-tag v="SN"/>
            <clip pos="1" part="a_nbr"/> 
            <clip pos="2" part="a_pers"/> 
            <clip pos="2" part="a_possessive"/>
             <lit-tag v="nom"/>
